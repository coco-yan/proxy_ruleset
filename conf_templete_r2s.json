{
    "log": {
        "level": "warn",
        "timestamp": true
    },
    "dns": {
        "servers": [
            {
                "tag": "default-dns",
                "type":"udp",
                "server": "183.221.253.100"
            },
            {
                "tag": "google",
                "type": "https",
                "server": "8.8.8.8",
                "detour": "节点选择"
            }
        ],
        "rules": [
            {
                "clash_mode": "direct",
                "server": "default-dns"
            },
            {
                "clash_mode": "global",
                "server": "google"
            },
            {
                "rule_set": ["geosite-cn", "site_defined"],
                "server": "default-dns"
            },
            {
                "server":"google",
                "strategy": "ipv4_only"
            }
        ],
            "strategy": "prefer_ipv4"
    },
    "inbounds": [
        {
            "type": "tun",
            "tag": "tun-in",
            "address": [
                "172.19.0.1/30",
                "2001:0470:f9da:fdfa::1/64"
            ],
            "mtu": 9000,
            "auto_route": true,
            "auto_redirect": true,
            "strict_route": true,
            "route_exclude_address_set": ["ip_private", "geoip-cn"]
        }
    ],
    "outbounds": [
        {
            "type": "direct",
            "tag": "direct-out"
        },
        {
            "type": "selector",
            "tag": "GLOBAL",
            "outbounds": [
              "direct-out",
              "节点选择",
              "手动切换"
            ],
            "default": "手动切换"
        },
        {
            "tag": "节点选择",
            "type": "selector",
            "outbounds": [
                "🇭🇰 香港自动",
                "🇭🇰 香港节点",
                "🇨🇳 台湾节点",
                "🇺🇸 美国节点",
                "🇸🇬 新加坡节点",
                "手动切换",
                "direct-out"
            ]
        },
        {
            "tag": "手动切换",
            "type": "selector",
            "outbounds": [
                "🇨🇳 台湾节点",
                "🇺🇸 美国节点",
                "🇸🇬 新加坡节点",
                "{all}"
            ], 
            "filter": [{ "action": "exclude", "keywords": ["官网|异常|剩余|到期|时间|有效"] }]
        },
        {
            "tag": "💬 AI",
            "type": "selector",
            "default": "手动切换",
            "outbounds": [
              "节点选择",
              "手动切换"
            ]
        },
        {
            "tag": "🪟 Microsoft",
            "type": "selector",
            "outbounds": [
              "direct-out",
              "节点选择"
            ]
        },
        {
            "tag": "🍎 Apple",
            "type": "selector",
            "outbounds": [
              "direct-out",
              "节点选择"
            ]
        },
        {
            "tag": "🇬 Google",
            "type": "selector",
            "outbounds": [
              "节点选择",
              "🇭🇰 香港节点",
              "🇨🇳 台湾节点",
              "🇺🇸 美国节点",
              "🇸🇬 新加坡节点"
            ]
        },
        {
            "tag": "🐟 漏网之鱼",
            "type": "selector",
            "outbounds": [
              "节点选择",
              "手动切换",
              "direct-out"
            ]
        },
        {
            "tag": "🇭🇰 香港自动",
            "type": "urltest",
            "outbounds": ["{all}"], "filter": [{ "action": "include", "keywords": ["🇭🇰|HK|hk|香港|港|HongKong"] }],
            "interval": "10m",
            "tolerance": 50
          },
        {
            "tag": "🇭🇰 香港节点",
            "type": "selector",
            "outbounds": ["{all}"], "filter": [{ "action": "include", "keywords": ["🇭🇰|HK|hk|香港|港|HongKong"] }]
        },
        {
            "tag": "🇨🇳 台湾节点",
            "type": "selector",
            "outbounds": ["{all}"], "filter": [{ "action": "include", "keywords": ["🇨🇳|CN|cn|TW|tw|台湾|台|Taiwan"] }] 
        },
        {
            "tag": "🇺🇸 美国节点",
            "type": "selector",
            "outbounds": ["{all}"], "filter": [{ "action": "include", "keywords": ["🇺🇸|US|us|美国|美|United States"] }]
        },
        {
            "tag": "🇸🇬 新加坡节点",
            "type": "selector",
            "outbounds": ["{all}"], "filter": [{ "action": "include", "keywords": ["🇸🇬|SG|新加坡|坡|狮城|Singapore"] }]
        }
    ],
    "route": {
        "default_domain_resolver": {
            "server": "default-dns",
            "client_subnet": "211.137.96.205"
        },
        "rules": [
            {
                "action": "sniff"
            },
            {
                "protocol": "dns",
                "action": "hijack-dns"
            },
            {
                "protocol": "quic",
                "action": "reject"
            },
            {
                "clash_mode": "direct",
                "outbound": "direct-out"
            },
            {
                "clash_mode": "global",
                "outbound": "GLOBAL"
            },
            {
                "ip_is_private": true,
                "outbound": "direct-out"
            },
            {
                "rule_set": [
